{
  "name": "leaguetracker",
  "description": "just for viewing stats",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/express/vite.config.ts",
    "build.hono": "vite build -c adapters/hono/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages deploy ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.express",
    "hono": "node server/entry.hono",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.7",
    "@builder.io/qwik-city": "^1.2.7",
    "@hono/node-server": "^1.1.1",
    "@sentry/browser": "^7.64.0",
    "@sentry/node": "^7.64.0",
    "@sentry/vite-plugin": "^2.7.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/compression": "^1.7.2",
    "@types/eslint": "8.40.0",
    "@types/express": "4.17.17",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "~5.60.1",
    "@typescript-eslint/parser": "~5.60.1",
    "autoprefixer": "^10.4.15",
    "compression": "^1.7.4",
    "eslint": "8.41.0",
    "eslint-plugin-qwik": "^1.2.7",
    "express": "4.18.2",
    "hono": "^3.5.0",
    "postcss": "^8.4.28",
    "prettier": "2.8.8",
    "rehype-pretty-code": "^0.10.0",
    "shiki": "^0.14.3",
    "tailwindcss": "^3.3.3",
    "typescript": "5.0.4",
    "undici": "5.22.1",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0"
  }
}
